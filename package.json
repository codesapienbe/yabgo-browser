{
  "name": "yabgo-browser",
  "version": "0.0.12",
  "description": "Yet Another Browser to Go and Visit - A minimal, gesture-driven Chromium browser with AI assistant and MCP integration",
  "main": "dist/main/main.js",
  "scripts": {
    "build": "echo 'Cleaning build artifacts...' && rm -rf dist release && echo 'Compiling TypeScript...' && npx tsc -p tsconfig.json && echo 'Bundling with webpack...' && npx webpack --mode production && npm run copy:assets",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:win": "npm run build && electron-builder --win",
    "dist:linux": "npm run build && electron-builder --linux",
    "start": "npx electron . --no-sandbox",
    "dev": "npm run build && npm start",
    "test": "npx jest",
    "clean": "echo 'Cleaning build artifacts...' && rimraf dist release .cache .parcel-cache .npm .yarn npm-debug.log* yarn-debug.log* yarn-error.log* && echo 'Clean complete!'",
    "copy:assets": "echo 'Copying assets...' && mkdir -p dist && cp src/renderer/index.html dist/ && cp src/renderer/styles.css dist/ && cp -r assets dist/assets || true && echo 'Assets copied!'",
    "cap:init": "npx cap init \"YABGO Browser\" io.codesapienbe.yabgo --web-dir=dist",
    "cap:add:android": "npx cap add android",
    "cap:install:android": "npm install @capacitor/android@^5.0.0",
    "cap:ensure:android": "npm run cap:install:android && npm run cap:add:android",
    "cap:copy": "npx cap copy",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "build:android": "npm run build && npm run cap:copy_and_sync && echo 'Now open Android Studio with `npx cap open android` to build the release APK'",
    "cap:copy_and_sync": "npx cap copy && npx cap sync android",
    "keystore:encode": "bash ./scripts/encode-keystore.sh release.keystore --copy",
    "keystore:props": "bash ./scripts/make-keystore-properties.sh",
    "check:secrets": "bash ./scripts/check-for-secrets.sh",
    "prepare": "husky install",
    "release": "bash ./scripts/release.sh",
    "release:patch": "bash ./scripts/release.sh patch --push",
    "release:minor": "bash ./scripts/release.sh minor --push",
    "release:major": "bash ./scripts/release.sh major --push"
  },
  "keywords": [
    "electron",
    "browser",
    "typescript",
    "chromium",
    "minimal",
    "gesture",
    "ai-assistant",
    "desktop-app"
  ],
  "author": {
    "name": "Codesapien Network",
    "email": "yilmaz@codesapien.net"
  },
  "license": "MIT",
  "homepage": "https://github.com/codesapienbe/yabgo-browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/codesapienbe/yabgo-browser.git"
  },
  "bugs": {
    "url": "https://github.com/codesapienbe/yabgo-browser/issues"
  },
  "devDependencies": {
    "@capacitor/android": "^5.7.8",
    "@capacitor/cli": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@types/electron": "^1.6.10",
    "@types/jest": "^29.5.8",
    "@types/jsdom": "^27.0.0",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "jsdom": "^27.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.2.2",
    "wait-on": "^7.2.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@modelcontextprotocol/server-everything": "^2025.1.14",
    "@modelcontextprotocol/server-filesystem": "^2025.1.14",
    "@modelcontextprotocol/server-memory": "^2025.1.14",
    "@modelcontextprotocol/server-sequential-thinking": "^2025.1.14",
    "marked": "^16.4.1"
  },
  "build": {
    "appId": "com.yabgo.browser",
    "productName": "YABGO Browser",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "!node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!node_modules/*.d.ts",
      "!node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "extraFiles": [
      {
        "from": "assets",
        "to": "assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "signtoolOptions": {
        "signingHashAlgorithms": [
          "sha256"
        ],
        "rfc3161TimeStampServer": "http://timestamp.digicert.com"
      }
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Network",
      "maintainer": "Codesapien Network <yilmaz@codesapien.net>",
      "vendor": "Codesapien Network",
      "synopsis": "Modern gesture-driven web browser",
      "description": "Yet Another Browser to Go and Visit - A lightning-fast, gesture-driven web browser with AI assistant and MCP integration",
      "desktop": {
        "entry": {
          "Name": "YABGO Browser",
          "Comment": "Yet Another Browser to Go and Visit",
          "Categories": "Network;WebBrowser;",
          "MimeType": "text/html;text/xml;application/xhtml+xml;x-scheme-handler/http;x-scheme-handler/https;"
        }
      }
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "YABGO Browser"
    },
    "appx": {
      "applicationId": "YABGOBrowser",
      "displayName": "YABGO Browser",
      "publisherDisplayName": "Codesapien Network",
      "identityName": "CodesapienNetwork.YABGOBrowser",
      "languages": [
        "en-US"
      ],
      "backgroundColor": "#1a1a1a",
      "showNameOnTiles": true,
      "addAutoLaunchExtension": false
    },
    "mac": {
      "category": "public.app-category.productivity",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "identity": null
    }
  }
}
